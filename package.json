{
    "name": "ci_foundation",
    "module": "index.ts",
    "type": "module",
    "private": true,
    "scripts": {
        "build": "rm -rf dist && bun build src/index.ts --outdir dist --target bun --minify",
        "start": "bun dist/index.js",
        "dev": "bun run --watch src/index.ts",
        "test": "bun test",
        "typecheck": "tsc --noEmit",
        "format": "prettier --write \"**/*.{ts,js,json,md}\"",
        "format:check": "prettier --check \"**/*.{ts,js,json,md}\""
    },
    "devDependencies": {
        "@types/bun": "latest",
        "prettier": "^3.8.0",
        "typescript": "^5.9.3"
    },
    "peerDependencies": {}
}
